{
    "name": "lstluck/think-mccwss",
    "description": "这是一个云控基座的的WebSocket服务！",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "lstluck",
            "email": "lstluck@890413.xyz"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-swoole": ">=5.0",
        "swoole/ide-helper": "^5.0.0",
        "topthink/framework": "^6.0|^8.0"
    },
    "require-dev": {
        "php": "^8.0",
        "ext-swoole": ">=5.0",
        "swoole/ide-helper": "^5.0.0",
        "topthink/framework": "^6.0|^8.0",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "mccwss\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "\\tests\\mccwss\\": "tests/"
        }
    },
    "extra": {
        "think": {
            "services": [
                "mccwss\\Service"
            ],
            "config": {
                "mccwss": "src/config/mccwss.php"
            }
        }
    }
}